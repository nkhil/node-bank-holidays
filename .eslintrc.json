{
  "extends": "airbnb-base",
  "plugins": ["import"],
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-console": "off",
    "no-underscore-dangle": "off",
    "strict": "off",
    "complexity": ["warn", 10]
  },
  "env": {
    "browser": false,
    "commonjs": true,
    "es6": true,
    "mocha": true
  },
  "space-in-parens": [1, "always"]
}
